<Page xmlns="http://www.nativescript.org/tns.xsd" loaded="pageLoaded">
  <StackLayout>
    <Button text="{{ flashlightState }}" tap="toggleFlashlight" />
    <ios>
      <Label text="intensity (iOS only)"/>
      <Slider id="intensitySlider" value="{{ intensity }}" maxValue="100" minValue="1" />
    </ios>
  </StackLayout>
</Page>